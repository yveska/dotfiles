      {
        "layer": "top",
        "position": "top",
	"height": 23,
	"spacing": 4,

        "modules-left": [
	  "hyprland/workspaces",
	  "custom/music",
        ],

        "modules-right": [
	  "pulseaudio",
	  "tray",
	  "clock#date",
	  "clock#time",
        ],

