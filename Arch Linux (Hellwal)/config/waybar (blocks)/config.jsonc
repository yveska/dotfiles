        "custom/music":{
          "format": "  ♪ {} ",
          "escape": true,
          "interval": 1,
          "tooltip": false,
          "exec": "playerctl metadata --format='{{ title }}'",
          "on-click": "playerctl play-pause",
          "max-length": 50,
        },

        "clock#time": {
          "interval": 10,
          "format": "{:%H:%M}",
          "tooltip": false,
        },

        "clock#date": {
          "interval": 20,
          "format": "{:%e %b %Y}",
          "tooltip": false,
        },

        "pulseaudio": {
        "scroll-step": 1, 
        "format": "{icon} {volume}%",
        "format-bluetooth": "{icon} {volume}%",
        "tooltip":"test",
        "format-icons": [
        "",
        "",
        " "
        ],
        "on-click": "pavucontrol",
        "format-muted": " ",
	"nospacing": 1   
	},

        "tray": {
          "icon-size": 18,
          "spacing": 10,
        },
      }
